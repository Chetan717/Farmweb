{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/CompUtils/Header/Header.jsx",
    "src/app/PopularProd/PopularProd.jsx",
    "src/app/ProductInfo/Comp/ProductDes.jsx",
    "src/app/ProductInfo/Comp/ImageSlide.jsx",
    "src/app/ProductInfo/MainDetails.jsx",
    "src/app/PopularProd/Comp/PopularProdList.jsx",
    "src/app/ProductInfo/Comp/MoreInfoComp/Review.jsx",
    "src/app/ProductInfo/Comp/MoreInfoComp/Doses.jsx",
    "src/app/ProductInfo/Comp/MoreInfoComp/Description.jsx",
    "src/app/ProductInfo/Comp/MoreInfoComp/AdditionalInfo.jsx",
    "src/app/ProductInfo/Comp/MoreInfo.jsx",
    "src/app/page.js",
    "src/app/layout.jsx",
    "src/app/ProductInfo/[product]/page.jsx",
    "src/app/ProductInfo/Comp/RelatedProduct/Related.js",
    "src/app/CompUtils/SearchPro/HandleSearch.jsx"
, "src/app/CartOperation/SubComp/AdressInput.jsx", "src/app/CartOperation/SubComp/Address.jsx", "src/app/CartOperation/CartMain.jsx", "src/app/specification/Specification.jsx"  ],
  "exclude": ["node_modules"]
}
